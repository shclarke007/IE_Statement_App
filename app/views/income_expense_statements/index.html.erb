<p id="notice"><%= notice %></p>

<h1>Income Expense Statements</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @income_expense_statements.each do |income_expense_statement| %>
      <tr>
        <td><%= link_to 'Show', income_expense_statement %></td>
        <td><%= link_to 'Edit', edit_income_expense_statement_path(income_expense_statement) %></td>
        <td><%= link_to 'Destroy', income_expense_statement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Income Expense Statement', new_income_expense_statement_path %>
